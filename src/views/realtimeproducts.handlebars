<div id="productsContainer" class="row row-cols-1 row-cols-md-2 g-4 productsContainer">
</div>


<h1>Ingresar nuevo producto</h1>
<form id="formId">
    <label for="title">Title</label>
    <input type="text" required name="title">
    <label for="description">Description</label>
    <input type="text" required name="description">
    <label for="price">Price</label>
    <input type="number" required name="price">
    <label for="code">Code</label>
    <input type="text" required name="code">
    <label for="stock">Stock</label>
    <input type="number" required name="stock">
    <label for="category">Category</label>
    <input type="text" required name="category">
    <label for="thumbnails">Thumbnails</label>
    <input type="text" name="thumbnails">

    <button type="submit">Cargar</button>
</form>